{%- if hide_resource == "true" -%}
  {% continue %}
{%- endif -%}
{%- comment -%} Wholesale_Club_Product_Visibility_For_Loop End {%- endcomment -%}
  

         <div class="child"  style="" >  
  <h3>{{ block.settings.linktextssfefe}}</h3>
 

    {% if block.settings.use_check %}
    <div class="youtube" style="display:none;">

    
      <iframe id="test{{ forloop.index }}" class="wrtch"  src="https://www.youtube.com/embed/{{ block.settings.vid }}"  style="
  {% if section.settings.headjghjghjg == "auto auto" %}   height: 310px;{% endif %}
   ;
" wmode="transparent" frameborder="0" allowfullscreen></iframe>

</iframe>
</div>
