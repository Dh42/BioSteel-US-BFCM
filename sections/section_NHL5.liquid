    width: 100%;
   
    height: 100%;
     
" />
    
     
                    
         
         
         
         </div> 


  <script type='text/javascript'>
$('.image').click(function() {
  startvideo = $(this).prev();
  
  $(this).hide();
  startvideo.show()
    
});
  </script>
{% else %}

<div class="youtube" >

      <iframe id="test{{ forloop.index }}"  src="https://www.youtube.com/embed/{{ block.settings.vid }}" wmode="transparent" frameborder="0" allowfullscreen 

