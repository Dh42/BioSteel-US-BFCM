  <div class="Section__container">

<div class="mainVido">
    <div class="headingCenter">
<{{ section.settings.heading_size1H1fhk }} style="
    text-align: center;
  color: {{ section.settings.heading_coddlor546}};
  font-size: {{ section.settings.font_size43ddtg6 }}px;
  padding-top: 50px;
">{{ section.settings.frist_text }}</{{ section.settings.heading_size1H1fhk }}>

