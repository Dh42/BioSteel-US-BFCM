    .feature-row{
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      width: 100%;
      -webkit-justify-content: space-between;
      -ms-justify-content: space-between;
      justify-content: space-between;
      -ms-flex-align: center;
      -webkit-align-items: center;
      -moz-align-items: center;
      -ms-align-items: center;
      -o-align-items: center;
      align-items: center;
    }
    .Section__container .feature-row__item{      
      -webkit-flex: 0 1 50%;
      -moz-flex: 0 1 50%;
      -ms-flex: 0 1 50%;
      flex: 0 1 50%;
    }
    .clearfix{
      clear:both;
    }
    .feature-row__text {
      padding-top: 35px;
      padding-bottom: 35px;
    }
    @media only screen and (min-width: 750px){
      .feature-row__text--left {
        padding-left: 35px;
      }
      .featured-row__subtext {
        font-size: 1.125em;
      }
    }
    #shopify-section-image-with-text{
      margin-top:50px;
    }
    .FeaturedColumns-inner{
      margin-top:50px !important;
      
    }
    @media only screen and (max-width: 749px){
      .feature-row {
        -webkit-flex-direction: column;
        -moz-flex-direction: column;
        -ms-flex-direction: column;
        flex-direction: column;
        display: block;
      }
      .feature-row__item {
        -webkit-flex: 1 1 auto;
        -moz-flex: 1 1 auto;
        -ms-flex: 1 1 auto;
        flex: 1 1 auto;
        max-width: 100%;
      }
      .feature-row__text {
        order: 2;
        padding-bottom: 0;
      }
    }
    .image_blocks{
      padding:0px !important;
    }
    .image_blocks .feature-row__item{
      background-size:100% 100% !important;
      
      /*background-size: inherit !important;
      background-repeat: no-repeat !important;*/
    }
  </style>
  
  {% if collection %}
  <script type="text/javascript">
    if(true){
      $(".bundle_add_cart_coll").click(function(){

        $(this).html("ADDING...");
        var variantIds = $(this).attr("variant_ids").split(",");

        var items = [];
        for(var key in variantIds){
          items.push({
            quantity:1,
            id:parseInt(variantIds[key])
          });
        }

        $.ajax({
          type: 'POST', 
          url: '/cart/add.js',
          data: {items:items},
          success: function(res){
            window.location.href = "/cart";
          },
          error: function(err){
            $(this).html("ADD TO CART");
            if(err && err.responseText){
              if(err.responseText.indexOf("description") !== -1){
                var responseObj = JSON.parse(err.responseText);
                if(responseObj.description){
                  alert(responseObj.description);
                }
                else{
                  alert(err.responseText);
                }
              }
              else{
                alert(err.responseText);
              }
            }
          }
       });
      });
    }
  </script>
  
  {% endif %}
  
