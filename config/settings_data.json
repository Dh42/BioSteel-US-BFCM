      "ShortDescription": {
        "type": "ShortDescription",
        "settings": {
          "show_short_description": true,
          "bottom_white_space": 65,
          "description_heading": "More Than Just Sports Nutrition",
          "heading_font_size": 25,
          "description_sub": "The BioSteel way begins with sports nutrition but encompasses a lifestyle that encourages everyone to be at their best. Whether it is recipes, training tips, healthy living advice or supplement deep dives, the BioSteel Blog is your resource for being better.",
          "description_font_size": 15
        }
